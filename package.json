{
  "name": "nexmo-cli",
  "version": "0.3.13",
  "description": "Nexmo Command Line Interface",
  "main": "lib/request.js",
  "scripts": {
    "prebuild": "eslint src",
    "build": "babel -d lib src/ -s inline",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pretest": "eslint tests",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=text-lcov mocha --require @babel/register tests/* > coverage.lcov",
    "test": "mocha --require @babel/register tests/*",
    "watch:test": "watch 'npm test' tests src",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nexmo/nexmo-cli.git"
  },
  "keywords": [
    "nexmo",
    "sms",
    "voice",
    "cli"
  ],
  "bin": {
    "nexmo": "lib/bin.js"
  },
  "author": "nexmo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nexmo/nexmo-cli/issues"
  },
  "contributors": [
    "cbetta",
    "leggetter"
  ],
  "homepage": "https://github.com/nexmo/nexmo-cli",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.2.0",
    "sinon-test": "^2.4.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "colors": "^1.3.2",
    "commander": "^2.19.0",
    "didyoumean": "^1.2.1",
    "ini": "^1.3.4",
    "nexmo": "^2.4.0"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
